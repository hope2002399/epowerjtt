<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.centit.poweritem.po.BpowerItem" table="b_power_item">

		<id name="itemId" type="java.lang.String">
			<column name="ITEM_ID" length="32" />
		</id>

		<property name="itemName" type="java.lang.String">
			<column name="item_name" length="500" />
		</property>
		<property name="itemType" type="java.lang.String">
			<column name="item_type" length="2" />
		</property>
		<property name="itemStatus" type="java.lang.String">
			<column name="item_status" length="1" />
		</property>
		<property name="parentId" type="java.lang.String">
			<column name="parent_id" length="32" />
		</property>
		<property name="parentName" type="java.lang.String">
			<column name="parent_name" length="500" />
		</property>
		<property name="isContainSub" type="java.lang.String">
			<column name="isContainSub" length="1" />
		</property>
		<property name="orgId" type="java.lang.String">
			<column name="orgid" length="10" />

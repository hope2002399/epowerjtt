		<property name="spObject" type="java.lang.String">
			<column name="sp_object" length="200" />
		</property>
		<property name="useLevel" type="java.lang.String">
			<column name="use_Level" length="200" />
		</property>
		<property name="useUnit" type="java.lang.String">
			<column name="use_unit" length="500" />
		</property>
		<property name="linkPhone" type="java.lang.String">
			<column name="link_phone" length="100" />
		</property>
		<property name="monitorPhone" type="java.lang.String">
			<column name="monitor_phone" length="100" />
		</property>
		<property name="httpUrl" type="java.lang.String">
			<column name="http_url" length="500" />
		</property>
		<property name="dealAddress" type="java.lang.String">
			<column name="deal_Address" length="500" />
		</property>
		<property name="dealTime" type="java.lang.String">
			<column name="deal_time" length="200" />
		</property>
		<property name="lawAccording" type="java.lang.String">
			<column name="law_According" />
		</property>
		<property name="remark" type="java.lang.String">
			<column name="remark" />
		</property>
		<property name="isNetwork" type="java.lang.String">
			<column name="isnetwork" length="1" />
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="create_time" length="7" />
		</property>
		<property name="modifyTime" type="java.util.Date">
			<column name="modify_time" length="7" />
		</property>
		<property name="itemQldyItemId" type="java.lang.String">
			<column name="item_qldy_powerid" length="50" />
		</property>
	</class>

</hibernate-mapping>
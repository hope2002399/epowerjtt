<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.centit.powerbase.po.RecordFileDep" table="s_recordFileDep">
<id name="no" type="java.lang.String">
			<column name="no" precision="10" scale="0" />
			<generator class="assigned" />
		</id>
		<property name="recordCode" type="java.lang.String">
			<column name="recordCode" length="50" />
		</property>
		<property name="constituteDepID" type="java.lang.String">
			<column name="constituteDepID" length="50" />
		</property>
  </class>
</hibernate-mapping>
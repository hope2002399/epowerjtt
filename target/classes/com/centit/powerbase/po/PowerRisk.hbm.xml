<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.centit.powerbase.po.PowerRisk" table="s_power_risk">
<id name="itemId" type="java.lang.String" >
	<column name="item_id" length="34"/>
</id>
<property name="riskType" type="java.lang.String">
	<column name="RISK_TYPE" length="300"/>
</property>
<property name="riskDescrib" type="java.lang.String">
	<column name="RISK_DESCRIB" length="2000"/>
</property>
<property name="riskControlWay" type="java.lang.String">
	<column name="RISK_CONTROL_WAY"/>
</property>
<property name="remark" type="java.lang.String">
	<column name="REMARK"/>
</property>
<property name="lastModifyDate" type="java.util.Date">
	<column name="LAST_MODIFY_DATE"/>
</property>
</class>

</hibernate-mapping>
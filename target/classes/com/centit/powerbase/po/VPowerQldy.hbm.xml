<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.centit.powerbase.po.VPowerQldy" table="v_power_qldy">
<id name="itemId" type="java.lang.String" >
	<column name="ITEM_ID" length="100"/>
</id>
<property name="otherItemId" type="java.lang.String">
	<column name="OTHER_ITEM_ID" length="100"/>
</property>
<property name="otherNbId" type="java.lang.String">
	<column name="OTHER_NB_ID" length="100"/>
</property>
<property name="itemName" type="java.lang.String">
	<column name="item_name" length="1000"/>
</property>
<property name="itemType" type="java.lang.String">
	<column name="item_type" length="100"/>
</property>
<property name="orgId" type="java.lang.String">
	<column name="org_id" length="100"/>
</property>

</class>

</hibernate-mapping>